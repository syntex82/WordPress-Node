    "@types/speakeasy": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.6",
